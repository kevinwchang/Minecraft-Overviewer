#!/bin/bash
cd "$( dirname "$0" )"
VERSION=1.7.9
wget -N https://s3.amazonaws.com/Minecraft.Download/versions/$VERSION/${VERSION}.jar -P ~/.minecraft/versions/$VERSION/
rsync -a --delete ../world .
python overviewer.py --config config.py
python overviewer.py --config config.py --genpoi

keychain id_rsa
. ~/.keychain/`uname -n`-sh
rsync -acvz --files-from=rsync-include -e ssh /var/www/minecraft/map/ nfsipm:/home/public/
